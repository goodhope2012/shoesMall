<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog/1.9" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog/1.9 http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-1.9.xsd">
	<preConditions>
		<dbms type="postgresql" />
	</preConditions>
	<property name="table.name" value="USER_BEHAVIOR" />
	<changeSet id="create.sequence" author="qh">
		<createSequence sequenceName="SEQ_USER_BEHAVIOR" startValue="10000" />
	</changeSet>
	<changeSet id="create.table" author="qh">
		<createTable tableName="${table.name}">
			<column name="ID" type="bigint">
				<constraints primaryKey="true" nullable="false" />
			</column>
			<column name="SHARE_KEY" type="VARCHAR(128)" />
			<column name="DATE" type="DATETIME" />
			<column name="IMEI" type="VARCHAR(256)" />
			<column name="ACTION" type="VARCHAR(1024)" />
			<column name="RESULT" type="VARCHAR(128)" />
			<column name="ADDITIONAL" type="VARCHAR(10240)" />
		</createTable>
	</changeSet>
</databaseChangeLog>